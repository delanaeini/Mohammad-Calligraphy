{
  "name": "portfolioproject",
  "version": "1.0.0",
  "description": "portfolioproject",
  "main": "index.html",
  "scripts": {
    "clean": "rimraf dist",
    "usemin": "usemin gallery.html -d dist --htmlmin -o dist/gallery.html && usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "imagemin": "imagemin images/* -o dist/images",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin",
    "lite": "lite-server",
    "start": "npm run lite",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delanaeini/Mohammad-Caligraphy.git"
  },
  "devDependencies": {
    "copyfiles": "^2.2.0",
    "imagemin-cli": "^5.1.0",
    "lite-server": "^2.6.1",
    "rimraf": "^3.0.2",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "bootstrap": "^4.5.2",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "package.json": "^2.0.1",
    "popper.js": "^1.16.1"
  },
  "author": "Dela Naeini",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/delanaeini/Mohammad-Caligraphy/issues"
  },
  "homepage": "https://github.com/delanaeini/Mohammad-Caligraphy#readme"
}
